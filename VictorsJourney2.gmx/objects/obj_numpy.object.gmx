        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Frame Logic

// Aim
if (!thrown) {

    theta += theta_speed;
    if (theta &gt;= 360) theta -= 360;
    if (obj_victor.image_xscale == 1) { // facing right
        x = obj_victor.x + lengthdir_x(r, theta);
    }
    else { // facing left
        x = obj_victor.x - lengthdir_x(r, theta);
    }
    if (obj_victor.image_yscale == 1) { // right side up
        y = obj_victor.y + lengthdir_y(r, theta);
    }
    else { // upside down
        y = obj_victor.y - lengthdir_y(r, theta);
    }
    
    if (obj_victor.image_xscale == 1 &amp;&amp; x == obj_victor.x + r
        || obj_victor.image_xscale == -1 &amp;&amp; x == obj_victor.x - r
        || obj_victor.image_yscale == 1 &amp;&amp; y == obj_victor.y - r
        || obj_victor.image_yscale == -1 &amp;&amp; y == obj_victor.y + r) {
        theta_speed *= -1;
    }
    
}
// Thrown
else {
    
    grav = obj_victor.grav;

    //show_debug_message("grav: " + string(grav));
    
    vsp += grav - (vsp/67);

    if (vcolliding(obj_numpy)) {
        //show_debug_message("numpy vcolliding");
    }
    
    if (hcolliding(obj_numpy)) {
        show_debug_message("numpy hcolliding");
    }
        
    if (!vcolliding(obj_numpy)) {
        on_floor = false;
        on_ceiling = false;
    }
    
    if (!hcolliding(obj_numpy)) {
        right_wall = false;
        left_wall = false;
    }
        
    if (on_floor || right_wall || left_wall || on_ceiling) {
        vsp = 0;
    }
    
    // Horizontal collision with wall
    if (place_meeting(x + hsp, y, obj_wall)) {
        if (sign(hsp) == -1) {
            left_wall = true;
        }
        else if (sign(hsp) == 1) {
            right_wall = true;
        }
        else {
            show_debug_message("hsp is zero.. not sure why.");
        }
        while (!place_meeting(x + sign(hsp), y, obj_wall)) {
            x += sign(hsp);
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Horizontal collision with dimension 1 object
    if (place_meeting(x + hsp, y, obj_dim1) &amp;&amp; obj_victor.dim == 1) {
        if (sign(hsp) == -1) {
            left_wall = true;
        }
        else if (sign(hsp) == 1) {
            right_wall = true;
        }
        else {
            show_debug_message("hsp is zero.. not sure why.");
        }
        while (!place_meeting(x + sign(hsp), y, obj_dim1)) {
            x += sign(hsp);
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Horizontal collision with dimension 2 object
    if (place_meeting(x + hsp, y, obj_dim2) &amp;&amp; obj_victor.dim == 2) {
        if (sign(hsp) == -1) {
            left_wall = true;
        }
        else if (sign(hsp) == 1) {
            right_wall = true;
        }
        else {
            show_debug_message("hsp is zero.. not sure why.");
        }
        while (!place_meeting(x + sign(hsp), y, obj_dim2)) {
            x += sign(hsp);
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Horizontal collision with disappearing block
    if (place_meeting(x + hsp, y, obj_dis) &amp;&amp; global.blocksVisible) {
        on_wall = true;
        if (sign(hsp) == -1) {
            left_wall = true;
        }
        else if (sign(hsp) == 1) {
            right_wall = true;
        }
        else {
            show_debug_message("hsp is zero.. not sure why.");
        }
        while (!place_meeting(x + sign(hsp), y, obj_dis)) {
            x += sign(hsp);
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Vertical collision with wall
    if (place_meeting(x, y + vsp, obj_wall)) {
        left_wall = false;
        right_wall = false;
        while (!place_meeting(x, y + sign(vsp), obj_wall)) {
            y += sign(vsp);
        }
        if (sign(vsp) == -1) {
            on_ceiling = true;
        }
        else if (sign(vsp) == 1) {
            on_floor = true;   
        }
        else {
            show_debug_message("vsp is zero... not sure what to do");
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Vertical collision with dimension 1 object
    if (place_meeting(x, y + vsp, obj_dim1) &amp;&amp; obj_victor.dim == 1) {
        left_wall = false;
        right_wall = false;
        while (!place_meeting(x, y + sign(vsp), obj_dim1)) {   
            y += sign(vsp);
        }
        if (sign(vsp) == -1) {
            on_ceiling = true;
        }
        else if (sign(vsp) == 1) {
            on_floor = true;   
        }
        show_debug_message("dim1");
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Vertical collision with dimension 2 object
    if (place_meeting(x, y + vsp, obj_dim2) &amp;&amp; obj_victor.dim == 2) {
        left_wall = false;
        right_wall = false;
        while (!place_meeting(x, y + sign(vsp), obj_dim2)) {
            y += sign(vsp);
        }
        if (sign(vsp) == -1) {
            on_ceiling = true;
        }
        else if (sign(vsp) == 1) {
            on_floor = true;   
        }
        else {
            show_debug_message("vsp is zero... not sure what to do");
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Vertical collision with disappearing object
    if (place_meeting(x, y + vsp, obj_dis) &amp;&amp; global.blocksVisible) {
        left_wall = false;
        right_wall = false;
        while (!place_meeting(x, y + sign(vsp), obj_dis)) {
            y += sign(vsp);
        }
        if (sign(vsp) == -1) {
            on_ceiling = true;
        }
        else if (sign(vsp) == 1) {
            on_floor = true;   
        }
        else {
            show_debug_message("vsp is zero... not sure what to do");
        }
        grav = 0;
        hsp = 0;
        vsp = 0;
    }
    
    // Out of bounds collision
    if (place_meeting(x, y, obj_oob)) {
        obj_victor.numpy_out = false;
        instance_destroy();
    }
    
    if (on_floor) {
        if (sign(obj_victor.grav) == 1) {
            sprite_index = spr_numpy_sit;
        }
        else {
            sprite_index = spr_numpy_flying;
        }
    }
    else if (left_wall) {
        if (sign(obj_victor.grav) == 1) {
            sprite_index = spr_numpy_cling;
        }
        image_xscale = -1;
    }   
    else if (right_wall) {
        sprite_index = spr_numpy_cling;       
    }
    else if (on_ceiling) {
        if (sign(obj_victor.grav) == 1) {
            sprite_index = spr_numpy_flying;
        }
        else {
            sprite_index = spr_numpy_sit;
        }
    }
    image_yscale = sign(obj_victor.grav);
    
    x += hsp;
    y += vsp;
    
    if (place_meeting(x, y, obj_victor)) {
        obj_victor.numpy_out = false;
        obj_health.image_index = 0;
        instance_destroy();
    }
    
}

if (global.reset_numpy) {
    obj_victor.numpy_out = false;
    global.reset_numpy = false;
    instance_destroy();
}
</string>
